/* pages/home/home.wxss */
.content-view {
  width: 100%;
  height: 100%;
  background: #fbf8d4;
}

.bg-view {
  position: relative;
  width: 100%;
  height: 100%;
}

.bg-top {
  width: 244rpx;
  height: 190rpx;
  position: absolute;
  top: 0;
  right: 0;
}

.bg-bottom {
  width: 750rpx;
  height: 251rpx;
  position: absolute;
  left: 0;
  bottom: 0
}

.input-view {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 100;
  position: absolute;
  left: 74rpx;
  top: 10%;
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
  width: 602rpx;
  height: 770rpx;
}

.input-view text {
  font-size: 20pt;
  width: 100%;
  text-align: center;
}

.input-name {
  width: 76%;
  height: 88rpx;
  background: #ffe5e5;
  text-align: center;
  color: #333;
  margin-top: 40rpx;
}

.input-pl {
  color: #d38484;
}

.radio-group {
  width: 90%;
  text-align: center;
  margin-top: 34rpx;
}
.radio-right {
  margin-left: 40rpx;
}

.create-btn {
  width: 76%;
  font-size: 14pt;
  text-align: center;
  border: 3px #e9423f;
  color: white;
  overflow: hidden;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 56rpx;
  background: linear-gradient(#e55e53, #e63c3b, #d42323);
}

.create-hover {
  background-color: #d32323;
}